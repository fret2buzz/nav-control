{
  "name": "navControl",
  "version": "2.0.0",
  "description": "navigation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx live-server",
    "watch": "npx sass --load-path=node_modules scss/main.scss css/main.css -w",
    "dev": "npx sass --load-path=node_modules scss/main.scss css/main.css --no-source-map",
    "build": "npm run dev && npm run prefix",
    "prefix": "npx postcss css/main.css --use autoprefixer --replace"
  },
  "author": "fret2buzz",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "live-server": "^1.2.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.58.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 255
  },
  "browserslist": [
    "last 2 versions",
    "> 3%"
  ]
}
